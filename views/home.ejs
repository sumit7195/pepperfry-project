    var container = document.getElementById("first")
    function slideImage() {
        var counter = 0;

        setInterval(function () {

            container.innerHTML = null;

            if (counter === images.length) {
                counter = 0;
            }
            var img = document.createElement("img");

            img.src = images[counter];

            container.append(img);

            counter++
        }, 2000);
    }

    slideImage()


    


